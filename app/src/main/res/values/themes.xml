<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.StayFit" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/yelloColor</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/black</item>
        <!-- Customize your theme here. -->
    </style>


    <style name="Theme.AppCompat.BottomNavBar" parent="Theme.MaterialComponents.DayNight.NoActionBar">

        <item name="colorPrimary">@color/white</item>
        <item name="colorOnPrimary">@color/black</item>
        <item name="backgroundTint">@color/yelloColor</item>

        </style>



</resources>